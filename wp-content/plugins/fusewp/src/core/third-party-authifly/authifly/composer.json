{
  "name": "mailoptin/authifly",
  "description": "PHP OAuth Authentication Library",
  "keywords": [
    "oauth",
    "openid",
    "authentication",
    "authorization",
    "social",
    "api"
  ],
  "type": "wordpress-plugin",
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "ext-curl": "*"
  },
  "autoload": {
    "psr-4": {
      "Authifly\\": "src/"
    },
    "classmap": [
      "OAuthCredentialStorage.php",
      "ConstantContact.php",
      "ConstantContactV3.php",
      "Aweber.php",
      "Aweber2.php",
      "CampaignMonitor.php",
      "VerticalResponse.php",
      "Facebook.php",
      "Hubspot.php",
      "Zoho.php",
      "Infusionsoft.php",
      "CleverReach.php",
      "Stripe.php",
      "Newsman.php",
      "Mailchimp.php",
      "Salesforce.php",
      "Microsoft.php",
      "HighLevel.php",
      "Google.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
